@model Setting
@{
    ViewData["Title"] = "Details";
}

@{
    ViewData["Title"] = "Detail";
}

<div class="m-4">
    <div>
        <h4 class="mt-3">Key</h4>
        <p>@Model.Key</p>
    </div>
    <div>
        <h4 class="mt-3">Value</h4>
        <p>
            @Model.Value
        </p>
    </div>
    <div>
        <h4>
            @if (@Model.URL == null)
            {
                @(" ")
                ;
            }
            else
            {
                @("URL")
                ;
            }
        </h4>
        <p>@Model.URL</p>
    </div>
    <div>
        <a asp-controller="setting" asp-action="index" class="btn btn-success mt-2">Geri</a>
    </div>
</div>

