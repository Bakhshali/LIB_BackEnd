@model Branch
@{
    ViewData["Title"] = "Delete";
}

<div class="m-4">
    <div>
        <h4 class="mt-3">Name</h4>
        <p>@Model.Name</p>
    </div>
     <div>
        <h4 class="mt-3">Location</h4>
        <p>@Model.Location</p>
    </div>
     <div>
        <h4 class="mt-3">PhoneNumber</h4>
        <p>@Model.PhoneNumber</p>
    </div>
     <div>
        <h4 class="mt-3">OfficeNumber</h4>
        <p>@Model.OfficeNumber</p>
    </div>
     <div class="mt-3">
        <h4 class="mt-3">Map</h4>
        <p><iframe src="@Model.Map" width="500" height="300" style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe></p>
    </div>
    <div>
        <form method="post">
            <button  class="btn btn-danger mt-2">Delete</button>
        </form>
    </div>
</div>

